          <activeWhen>
              <reference definitionId="org.jboss.tools.ui.structuredEditor"/>
          </activeWhen>
          <enabledWhen>
              <reference definitionId="org.jboss.tools.ui.structuredEditor"/>
          </enabledWhen>
       </handler>
       <handler
             class="org.eclipse.wst.xml.ui.internal.handlers.RemoveBlockCommentHandler"
