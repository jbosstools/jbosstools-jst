      <editorContribution
            id="org.jboss.tools.jst.jsp.cssClassDialogEditorContribution"
            targetID="org.eclipse.wst.css.core.csssource.source">
         <menu
               id="org.jboss.tools.jst.jsp.cssClassDialogSubMenu"
