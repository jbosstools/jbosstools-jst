             commandId="org.eclipse.wst.sse.ui.toggle.comment">
          <activeWhen>
              <reference definitionId="org.jboss.tools.ui.structuredEditor"/>
          </activeWhen>
          <enabledWhen>
              <reference definitionId="org.jboss.tools.ui.structuredEditor"/>
          </enabledWhen>
